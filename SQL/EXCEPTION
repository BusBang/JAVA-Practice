-----------
---EXCEPTION

DECLARE
    USERINFO EMPLOYEE%ROWTYPE;
    I NUMBER :=1;
BEGIN
    LOOP
        SELECT *
        INTO USERINFO
        FROM EMPLOYEE WHERE EMP_ID=200+I;
        DBMS_OUTPUT.PUT_LINE(I||'번 / 이름 : '||USERINFO.EMP_NAME);
        I := I+1;
    END LOOP;
EXCEPTION
    WHEN NO_DATA_FOUND
    THEN DBMS_OUTPUT.PUT_LINE('데이터가 없습니다.');
END;
/

